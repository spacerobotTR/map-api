<p id="notice"><%= notice %></p>

<h1>Incidents</h1>

<table>
  <thead>
    <tr>
      <th>Street</th>
      <th>City</th>
      <th>Zip</th>
      <th>Dayofweek</th>
      <th>Timeofday</th>
      <th>State</th>
      <th>Incidenttype</th>
      <th>Drnum</th>
      <th>Weatherevent</th>
      <th>Specialevent</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Comments</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @incidents.each do |incident| %>
      <tr>
        <td><%= incident.street %></td>
        <td><%= incident.city %></td>
        <td><%= incident.zip %></td>
        <td><%= incident.dayofweek %></td>
        <td><%= incident.timeofday %></td>
        <td><%= incident.state %></td>
        <td><%= incident.incidenttype %></td>
        <td><%= incident.drnum %></td>
        <td><%= incident.weatherevent %></td>
        <td><%= incident.specialevent %></td>
        <td><%= incident.latitude %></td>
        <td><%= incident.longitude %></td>
        <td><%= incident.comments %></td>
        <td><%= link_to 'Show', incident %></td>
        <td><%= link_to 'Edit', edit_incident_path(incident) %></td>
        <td><%= link_to 'Destroy', incident, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Incident', new_incident_path %>
